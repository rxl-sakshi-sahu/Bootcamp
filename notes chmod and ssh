*******************************************

<input type="checkbox" id="foregroundQuery" />
<input type="checkbox" id="selectedDatasheet" />
<input type="hidden" id="prevForegroundQueryState" />
<input type="hidden" id="prevSelectedDatasheetState" />
<input type="text" id="dataMiningVariableValue" />

<script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
<script>
    $(document).ready(function () {
        // Set initial state
        updateCheckboxStates();

        // Listen for changes in the dataMiningVariableValue
        $("#dataMiningVariableValue").on("change", function () {
            updateCheckboxStates();
        });

        function updateCheckboxStates() {
            var dataMiningVariableValue = $("#dataMiningVariableValue").val();

            // Get the previous states from hidden fields
            var prevForegroundQueryState = $("#prevForegroundQueryState").val();
            var prevSelectedDatasheetState = $("#prevSelectedDatasheetState").val();

            // Check if #dataMiningVariableValue is null or empty
            if (dataMiningVariableValue == null || dataMiningVariableValue.trim() === "") {
                // Restore the previous state of the checkboxes
                $("#foregroundQuery").prop("checked", prevForegroundQueryState === "true");
                $("#selectedDatasheet").prop("checked", prevSelectedDatasheetState === "true");
            } else {
                // Update the hidden fields with the current checkbox states
                $("#prevForegroundQueryState").val($("#foregroundQuery").prop("checked"));
                $("#prevSelectedDatasheetState").val($("#selectedDatasheet").prop("checked"));
            }
        }
    });
</script>

