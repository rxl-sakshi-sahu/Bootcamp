class Employee extends Person{
	Integer empId, salary;
	String company;
	Employee(String name,String gender,String address,Integer age,Integer empId,Integer salary,String company)
    {
      this.empId= empId;
      this.salary= salary;
      this.company= company;
    }
	Employee e= new Employee("abc","female","Delhi",21,2133,60000,xyz);
	
	println(e.empId);
	println(e.salary);
	println(e.@salary);
	
}

Q.3)
Sol-
 public static void patternn(int n)
    {
        int k;
        for(int i=0;i<=n;i++)
        {
            k = 1<<i;
        for(int j=1;j<=k;j++)
        {
        System.out.print("* ");
        }
        System.out.println("\n");}
    }
    // passing int n=3;
    
